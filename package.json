{
  "name": "puppeteer-service",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "rm -rf dist && tsc",
    "start": "pnpm build && node dist/index.js",
    "start:pm2": "pnpm build && pm2 start ecosystem.config.js"
  },
  "dependencies": {
    "ali-oss": "^6.23.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "puppeteer": "^17.1.3",
    "puppeteer-cluster": "^0.23.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
